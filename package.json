{
    "scripts": {
        "css": "npm-run-all --sequential css:sass css:postcss:tailwind css:postcss:autoprefixer",
        "css:dev": "npm-run-all --sequential css:sass:dev css:postcss:tailwind",
        "css:watch": "onchange ./_scss/*.scss ./_scss/**/*.scss --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run css:dev\" --title \"css:watch\" --message \"build failed\" --sound mute",
        "css:sass": "node-sass --output-style compressed ./_scss/style.scss ./_build/bundle.css",
        "css:sass:dev": "node-sass --source-map true ./_scss/style.scss ./_build/bundle.css",
        "css:postcss:autoprefixer": "postcss --use autoprefixer --no-map --output ./_build/bundle.css ./_build/bundle.css",
        "css:postcss:tailwind": "cross-env TAILWIND_MODE=build postcss --use tailwindcss cssnano --no-map --output ./_build/bundle.css ./_build/bundle.css",
        "js": "npm-run-all --sequential js:browserify js:minify",
        "js:dev": "npm-run-all --sequential js:browserify",
        "js:watch": "onchange ./_js/*.js ./_js/**/*.js ./_vue/*.vue ./_vue/**/*.vue ./_libs/*.js ./_libs/**/*.js --initial --poll 50 --delay 0 --await-write-finish 50 -- onerror \"npm run js:dev\" --title \"js:watch\" --message \"build failed\" --sound mute",
        "js:browserify": "browserify ./_js/script.js --outfile ./_build/bundle.js --debug -g [ browserify-css --minify=true --stripComments=true ] -t [ browserify-shim --global ] --transform babelify --transform vueify",
        "js:minify": "terser --compress --mangle --output ./_build/bundle.js ./_build/bundle.js",
        "browsersync": "from-env browser-sync start --reload-debounce 3000 --proxy DOMAIN --files \"./_build/*.html, ./_build/*.css, ./_build/*.js\"",
        "prod": "cross-env NODE_ENV=production npm-run-all --sequential css js",
        "dev": "cross-env NODE_ENV=development npm-run-all --parallel css:watch js:watch browsersync"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
